{
  "makefile.extensionOutputFolder": "./tmp",
  "makefile.configurationCachePath": "tmp/configurationCache.log",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "editor.formatOnSave": true,
  "cSpell.words": [
    "bwmarrin",
    "dbname",
    "dbtx",
    "discordgo",
    "feato",
    "gimei",
    "mattn",
    "PGHOST",
    "PGSSLMODE",
    "PGUSER",
    "sqlc",
    "Usecase"
  ]
}
